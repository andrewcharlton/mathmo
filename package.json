{
  "name": "MathMo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.1",
    "react-scripts-ts": "3.1.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "fmt": "./node_modules/prettier/bin-prettier.js --write 'src/**/*.ts'",
    "fmt-errors": "./node_modules/prettier/bin-prettier.js --list-different 'src/**/*.ts'",
    "lint": "tslint 'src/**/*.ts*'"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.2",
    "@types/react": "^16.4.18",
    "@types/react-bootstrap": "^0.32.14",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "@types/redux-logger": "^3.0.6",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "prettier": "^1.14.3",
    "redux-logger": "^3.0.6",
    "typescript": "^3.1.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/index.ts*",
      "!resources.d.ts",
      "!src/setupTests.ts",
      "!src/models/**/*"
    ],
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1"
    }
  }
}
